<div class="ui-body ui-body-a ui-corner-all">
  <%= form_for(@entry) do |f| %>
    <div class="field ui-field-contain">
      <%= f.label :expected_at %>
      <%= f.datetime_select :expected_at, {}, data: { role: "none"} %>
    </div>
    <div class="field ui-field-contain">
      <%= f.label :price %>
      <%= f.number_field :price %>
    </div>
    <%= hidden_field_tag :force_commit, true %>
    <%= f.submit t('Commit') %>
  <% end %>
</div>
<br>
